package 정처기실기코드;

class A{
	int a;
	int b;
}

public class ex17{
	static void func1(A m) {
		m.a*=10;
	}
	
	static void func2(A m) {
		m.a+=m.b;
	}
}


public class ex17 {

	public static void main(String[] args) {
		
		a m = new A();
		m.a = 100;
		func1(m);
		m.b = m.a;
		func2(m);
		System.out.println("%d", m.a);
		
	}

}
