package 정처기실기코드;

public class ex22 {

	public static void main(String[] args) {
		
		int a=0;
		for(int i=1;i<999;i++) {
			if(i%3==0 && i%2!=0)
				a=i;
		}
		System.out.println(a);
		
		for(int i=1;i<999;i++) {
			if(i%3==0)
				a=i;
		}
		System.out.println(a);
		
		for(int i=1;i<999;i++) {
			if(i%3==0)
				a=i;
		}
		System.out.println(a);
	}

}
