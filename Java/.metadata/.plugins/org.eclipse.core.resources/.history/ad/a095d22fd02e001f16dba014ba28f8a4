package JDBC기초;

import java.util.Scanner;

public class Ex01Insert {

	public static void main(String[] args) {
		
		// 사용자로부터 이름, 나이, 성별, 학과를 입력받아서
		// 입력받은 내용이 데이터베이스에 저장 될 수 있도록 만들어주자!
		Scanner sc = new Scanner(System.in);
		
		System.out.print("이름 : ");
		sc.next();
		System.out.print("나이 : ");
		sc.nextInt();
		System.out.print("성별 : ");
		sc.next();
		System.out.print("학과 : ");
		sc.next();
		
		
		
	}

}
